                  <Center />
                  <AlignRight />
                  <Font />
                  <Background />
                  <H1 />
                  <H2 />
                  <H3 />
                  <H4 />
                  <H5 />
                  <H6 />
                  <Abbreviation />
                </DisallowedActions>
                <DisallowedStyles />
              </FormattingFeatures>
            </template>
          </stylesheet>
            </tcm:FilterXSLT>
          </xsd:appinfo>
        </xsd:annotation>
      </xsd:element>
